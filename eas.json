{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "APP_ENV": "development",
        "API_URL": "https://localhost:3000/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "APP_ENV": "production",
        "API_URL": "https://nestjs-react-app-demo.up.railway.app/api"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "APP_ENV": "production",
        "API_URL": "https://nestjs-react-app-demo.up.railway.app/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
